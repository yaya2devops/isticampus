{
  "name": "stimulus-carousel",
  "version": "5.0.1",
  "description": "I used Stimulus controller to deal with ISTIC assets.",
  "keywords": [
    "stimulus",
    "stimulusjs",
    "stimulus controller",
    "carousel",
    "sliders",
    "swiperjs",
    "Swiper"
  ],
  "scripts": {
    "format": "prettier-standard  '**/*.{ts,css,html}' --format",
    "lint": "prettier-standard  '**/*.{ts,css,html}' --lint",
    "dev": "vite",
    "prod": "vite build --mode netlify",
    "build": "tsc --noEmit && vite build",
    "version": "yarn build",
    "np": "np --no-tests --no-2fa"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@hotwired/stimulus": "^3.2.1",
    "autoprefixer": "^10.4.13",
    "np": "^7.6.2",
    "postcss": "^8.4.20",
    "prettier-standard": "16.4.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.2.1"
  },
  "dependencies": {
    "stimulus-carousel": "^5.0.1",
    "swiper": "^8.4.5"
  }
}
